//! kan_resource_font_library_t
+categories {
    script = "Latn"
    used_for_languages = en
    precache_utf8_horizontal = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789. "
    
    +styles {
        font_data_file = "OpenSans-VariableFont_wdth,wght.ttf"
        variable_font_axes = 400.0, 100.0
    }
    
    +styles {
        style = bold
        font_data_file = "OpenSans-VariableFont_wdth,wght.ttf"
        variable_font_axes = 700.0, 100.0
    }
}
